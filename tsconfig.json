{
    "compilerOptions": {
        "outDir": "./scripts-generated",
        "target": "esnext",
        "lib": [
            "esnext"
        ],
        "moduleResolution": "node",
        "types": [
            "@aoemods/aoetypes"
        ],
        "strict": true,
        "rootDir": "scripts"
    },
    "tstl": {
        "luaTarget": "5.3",
        "sourceMapTraceback": false,
        "noHeader": true,
        "noImplicitSelf": true,
        "luaBundle": "main.lua",
        "luaBundleEntry": "scripts/main.ts"
    },
    "include": [
        "scripts/**/*.ts",
    ]
}